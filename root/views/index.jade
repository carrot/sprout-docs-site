extends layout

block content
  <% for(var i=0; i<menu_items.length; i++) { %>
  .intro
    h1#<%= S(menu_items[i]).slugify().value() %> <%= S(menu_items[i]).titleize().value() %> 
    :markdown
      Intro copy for <%= S(menu_items[i]).titleize().value() %> section
    
  section
    :markdown
      Rest of the documentation for <%= S(menu_items[i]).titleize().value() %> section
  
  <% } %>
